package calculator;

public class CalcLogic {
	public static int logicmethod(int num1, String op, int num2){
		int res = 0;
		switch(op){
		case "+": res = num1 + num2;
		break;
		case "-": res = num1 - num2;
		break;
		case "/": res = num1 / num2;
		break;
		case "*": res = num1 * num2;
		break;
		default: System.out.println("Not a number or available operation. Please check!");
		}
		return res;
	}
}
